;output = zeroonetwothreefourfivesixseveneightnine
(include "core/macro.cons")
(include "inc/io.cons")

(def int-name (x)
  (cond ((= x 0)
         $"zero")
        ((= x 1)
         $"one")
        ((= x 2)
         $"two")
        ((= x 3)
         $"three")
        ((= x 4)
         $"four")
        ((= x 5)
         $"five")
        ((= x 6)
         $"six")
        ((= x 7)
         $"seven")
        ((= x 8)
         $"eight")
        ((= x 9)
         $"nine")
        (else $">=10")))

(let loop ((i 0))
  (if (>= i 10)
    (printf $"\n")
    (begin
      (printf $"%s" (int-name i))
      (loop (+ i 1)))))

